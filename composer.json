{
  "name": "kubernetes/php-runtime",
  "description": "Runtime for kubernetes/php-client",
  "license": "MIT",
  "keywords": [
    "kubernetes",
    "docker",
    "runtime"
  ],
  "authors": [
    {
      "name": "Allan Sun",
      "email": "allan.sun@bricre.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "guzzlehttp/guzzle": "~6.0",
    "monolog/monolog": "@stable",
    "symfony/property-info": "~4|~5",
    "phpdocumentor/reflection-docblock": "^3.0|^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~9"
  },
  "conflict": {
    "phpdocumentor/reflection-docblock": "<3.2.2"
  },
  "autoload": {
    "psr-4": {
      "KubernetesRuntime\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KubernetesRuntime\\Tests\\": "tests"
    }
  },
  "prefer-stable": true
}
